# Environment Variables for GovCom Solutions

# API Keys
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# AWS Amplify CI/CD Configuration
# Copy this file to .env and fill in your values

# AWS Amplify Webhook URL (get this from Amplify Console)
AMPLIFY_WEBHOOK_URL=https://webhooks.amplify.us-east-1.amazonaws.com/action?token=your-token-here

# AWS Region (optional, defaults to us-east-1)
AWS_REGION=us-east-1

# Branch to deploy (optional, defaults to main)
DEFAULT_BRANCH=main

# Build timeout in minutes (optional, defaults to 10)
BUILD_TIMEOUT=10
